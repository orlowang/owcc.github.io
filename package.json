{
  "name": "react-static-blog",
  "version": "1.0.0",
  "description": "static blog by react",
  "main": "index.js",
  "scripts": {
    "start": "webpack && webpack-dev-server --devtool eval --progress --colors --hot --content-base build",
    "watch": "webpack-dev-server --devtool eval --progress --colors --hot --content-base src/assets/",
    "build": "webpack --progress --colors",
    "clean": "node bin/clean",
    "rbuild": "node bin/clean && webpack --progress --colors",
    "release": "node bin/clean && webpack --progress --colors --release",
    "deploy": "node autopost"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmkcc/react-static-blog.git"
  },
  "keywords": [
    "react",
    "static",
    "blog"
  ],
  "author": "Mark Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmkcc/react-static-blog/issues"
  },
  "homepage": "https://github.com/wmkcc/react-static-blog#readme",
  "devDependencies": {
    "animate.css": "^3.4.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.1.20",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "browser-sync": "^2.11.0",
    "browser-sync-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.0",
    "del": "^2.1.0",
    "exports-loader": "^0.6.2",
    "highlight.js": "^9.0.0",
    "history": "^1.13.1",
    "html-webpack-plugin": "^1.6.2",
    "imports-loader": "^0.6.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "marked": "^0.3.5",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0",
    "static-loader": "^0.1.8",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1",
    "whatwg-fetch": "^0.10.1"
  },
  "dependencies": {
    "react-addons-css-transition-group": "^0.14.3"
  }
}
